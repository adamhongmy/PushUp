<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.williams.asd" >
    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="10" />

    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <android:uses-permission
        android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        android:maxSdkVersion="18" />
    <android:uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <android:uses-permission
        android:name="android.permission.READ_EXTERNAL_STORAGE"
        android:maxSdkVersion="18" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TotalPushUp"
            android:configChanges="orientation"
            android:label="@string/title_activity_total_push_up"
            android:parentActivityName=".MainActivity" >
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.williams.asd.MainActivity" />
        </activity>
        <activity
            android:name=".Rest"
            android:label="@string/title_activity_rest"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.williams.asd.MainActivity" />
        </activity>
        <activity
            android:name=".TodayResult"
            android:label="@string/title_activity_today_result"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.williams.asd.MainActivity" />
        </activity>
        <activity
            android:name=".TodayGoal"
            android:label="@string/title_activity_today_goal"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.williams.asd.MainActivity" />
        </activity>
        <activity
            android:name=".Tutorial"
            android:label="@string/title_activity_tutorial"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.williams.asd.MainActivity" />
        </activity>
        <activity
            android:name=".CountUpPushup"
            android:label="@string/title_activity_count_up_pushup"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.williams.asd.MainActivity" />
        </activity>
        <activity
            android:name=".TodayGoalBeginner"
            android:label="@string/title_activity_today_goal_begineer"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.williams.asd.MainActivity" />
        </activity>
        <activity
            android:name=".ShowRecordActivity"
            android:label="@string/title_activity_show_record"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.williams.asd.MainActivity" />
        </activity>
        <activity
            android:name=".Notication"
            android:label="@string/title_activity_notication"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.williams.asd.MainActivity" />
        </activity>

        <service
            android:name=".MyAlarmService"
            android:enabled="true" />

        <receiver android:name=".MyReceiver" />


    </application>

</manifest>
